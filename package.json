{
  "name": "colorganize-app",
  "version": "1.0.8",
  "description": "Colorganize as an Electron app for desktop",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Lars / LGK",
  "license": "MIT",
  "build": {
    "appId": "com.colorganize",
    "productName": "Colorganize",
    "icon": "icon.ico",
    "mac": {
      "category": "public.app-category.graphics-design",
      "icon": "icon.icns"
    }
  },
  "devDependencies": {
    "electron": "^3.0.10",
    "electron-builder": "^20.38.2"
  },
  "dependencies": {
    "electron-config": "^2.0.0",
    "electron-default-menu": "^1.0.1"
  }
}